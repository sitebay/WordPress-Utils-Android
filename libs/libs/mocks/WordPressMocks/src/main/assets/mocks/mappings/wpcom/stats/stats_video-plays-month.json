{
    "request": {
        "method": "GET",
        "urlPathPattern": "/api/sites/([0-9]+)/stats/video-plays/",
        "queryParameters": {
            "period": {
                "equalTo": "month"
            },
            "max": {
                "matches": "[0-9]+"
            },
            "date": {
                "matches": "(.*)"
            },
            "locale": {
                "matches": "(.*)"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "date": "2019-07-31",
            "period": "month",
            "days": {
                "2019-07-01": {
                    "plays": [],
                    "other_plays": 0,
                    "total_plays": 0
                }
            }
        }
    }
}
